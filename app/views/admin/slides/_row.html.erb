<tr class="<%= cycle("even_row", "odd_row") -%>">
  <td><%=h row.title %></td>
  <td><%= link_to h(row.url), h(row.url) %></td>
  <td><%= link_to 'Show', object_path(row) %></td>
  <td><%= link_to 'Edit', edit_object_path(row) %></td>
  <td><%= link_to 'Destroy', object_path(row), :confirm => 'Are you sure?', :method => :delete %></td>
</tr>